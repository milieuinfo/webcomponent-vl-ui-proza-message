{
  "name": "vl-ui-proza-message",
  "version": "0.0.9",
  "main": "index.js",
  "description": "Component om een proza message te tonen en inline te updaten",
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "wct --config-file ./wct.local.conf.json",
    "test:unit:chrome": "wct -p -l chrome test/unit/*.test.html --npm",
    "test:e2e": "npm run test:server:start && npm run test:e2e:chrome && npm run test:e2e:firefox",
    "test:e2e:grid": "npm run test:server:start && npm run test:e2e:chrome:grid && npm run test:e2e:firefox:grid",
    "test:grid": "wct --config-file ./wct.docker.conf.json && npm run test:e2e:grid",
    "posttest:e2e": "pkill -f http-server",
    "test:e2e:chrome": "mocha --chrome ",
    "test:e2e:chrome:grid": "mocha  --chrome --grid",
    "test:e2e:firefox": "mocha --firefox",
    "test:e2e:firefox:grid": "mocha --firefox --grid",
    "test:e2e:opera": "mocha --opera",
    "test:e2e:safari": "mocha --safari",
    "test:server:start": "http-server &",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "minify *.src.js > vl-proza-message.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-proza-message/style.css' vl-proza-message.js",
    "release:prepare:commit": "git add -f vl-proza-message.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-proza-message.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-proza-message/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-proza-message#readme",
  "dependencies": {
    "tinymce": "5.1.5",
    "vl-ui-alert": "^1.3.9",
    "vl-ui-button": "^2.0.0",
    "vl-ui-core": "^3.1.1",
    "vl-ui-icon": "^2.1.1",
    "vl-ui-toaster": "^1.0.9",
    "vl-ui-typography": "^1.0.1"
  },
  "peerDependencies": {
    "tinymce": "5.1.5",
    "vl-ui-alert": "^1.3.9",
    "vl-ui-button": "^1.3.2",
    "vl-ui-core": "^3.1.1",
    "vl-ui-icon": "^2.1.1",
    "vl-ui-toaster": "^1.0.9",
    "vl-ui-typography": "^1.0.1"
  },
  "devDependencies": {
    "@govflanders/vl-ui-typography": "^3.9.1",
    "@govflanders/vl-ui-util": "^3.9.1",
    "vl-ui-util": "^3.4.4"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  }
}
